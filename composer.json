{
    "name": "tccl/router",
    "description": "A simple routing library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Gee",
            "email": "roger.gee@tulsalibrary.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "TCCL\\Router\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TCCL\\Test\\Router\\": "test/src/"
        }
    },
    "archive": {
        "exclude": [
            "/test/"
        ]
    },
    "require": {
        "php": ">=7.3.0",
        "ext-pcre": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
